<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Profesor" table="profesor">
        <id column="Id" name="id" type="integer">
            <generator class="identity"/>
        </id>
        <component name="nombre">
            <property name="nombre"></property>
            <property name="ape1"></property>
            <property name="ape2"></property>
        </component>
           
        <property name="tipoFuncionario">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">model.TipoFuncionario</param>
                <param name="type">12</param>
            </type>
        </property>
        

    </class>
</hibernate-mapping>
